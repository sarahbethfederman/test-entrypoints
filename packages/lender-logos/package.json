{
  "name": "@lendi-ui/lender-logos",
  "version": "2.2.1",
  "license": "ISC",
  "source": "src/index.tsx",
  "main": "dist/lender-logos.cjs.js",
  "module": "dist/lender-logos.esm.js",
  "files": [
    "dist",
    "*",
    "!(src|build|node_modules)"
  ],
  "preconstruct": {
    "entrypoints": [
      ".",
      "*",
      "!(dist|src|build|node_modules)"
    ]
  },
  "keywords": [],
  "author": "Lendi Devs",
  "sideEffects": false,
  "peerDependencies": {
    "@types/react": "^16.8",
    "@types/styled-components": "^4.1.18",
    "react": "^16.8",
    "styled-components": "^4"
  },
  "devDependencies": {
    "@babel/register": "^7.11.5",
    "@lendi-ui/asset-utils": "^1.0.0",
    "@svgr/cli": "^5.4.0",
    "@types/react": "^16.4.8",
    "@types/styled-components": "^4.1.18",
    "react": "^16.9.0",
    "styled-components": "^4"
  },
  "dependencies": {
    "@lendi-ui/utils": "^5.1.1"
  },
  "scripts": {
    "format:entrypoints": "prettier --write '**/package.json' --loglevel silent",
    "build:entrypoints": "node build/generate-entrypoints.js",
    "build:svg": "yarn svgr --silent --out-dir src/lender-logos-compiled src/lender-logos",
    "build": "yarn run build:svg && yarn run build:entrypoints && yarn run format:entrypoints"
  }
}
