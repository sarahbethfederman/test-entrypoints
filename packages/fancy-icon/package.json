{
  "name": "@lendi-ui/fancy-icon",
  "version": "3.2.0",
  "main": "dist/fancy-icon.cjs.js",
  "module": "dist/fancy-icon.esm.js",
  "source": "src/index.ts",
  "files": [
    "dist",
    "*",
    "!(src|build|node_modules)"
  ],
  "preconstruct": {
    "entrypoints": [
      ".",
      "*",
      "!(dist|src|build|node_modules)"
    ]
  },
  "keywords": [],
  "author": "Lendi Devs",
  "license": "ISC",
  "sideEffects": false,
  "peerDependencies": {
    "@lendi-ui/theme": "^9.1.0",
    "@types/react": "^16.8",
    "@types/styled-components": "^4.1.18",
    "react": "^16.8",
    "styled-components": "^4"
  },
  "devDependencies": {
    "@babel/register": "^7.11.5",
    "@lendi-ui/asset-utils": "^1.0.0",
    "@lendi-ui/spacing": "^7.1.1",
    "@lendi-ui/theme": "*",
    "@lendi-ui/typography": "^5.1.1",
    "@svgr/cli": "^5.4.0",
    "@types/react": "^16.4.8",
    "@types/styled-components": "^4.1.18",
    "react": "^16.9.0",
    "styled-components": "^4"
  },
  "dependencies": {
    "@lendi-ui/color": "^5.1.1"
  },
  "scripts": {
    "format:entrypoints": "prettier --write '**/package.json' --loglevel silent",
    "build:entrypoints": "node build/generate-entrypoints.js",
    "build:svg": "yarn svgr --silent --out-dir src/fancy-icons-compiled src/fancy-icons",
    "build": "yarn run build:svg && yarn run build:entrypoints && yarn run format:entrypoints"
  }
}
